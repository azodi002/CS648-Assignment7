{
  "extends": "airbnb-base",
  "env": {
    "node": "true"
  },
  rules: {
    "no-console": "off",
    "no-tabs": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-unused-vars" : 0,
    "max-len": 0,
    "no-alert": 0,
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "import/extensions": [ "error", "always", { "ignorePackages":true } ],
    "react/prop-types": "off",
  }
}